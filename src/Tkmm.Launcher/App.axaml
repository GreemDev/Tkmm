<Application x:Class="Tkmm.Launcher.App"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sty="using:FluentAvalonia.Styling"
             RequestedThemeVariant="Dark">
    <Application.Styles>
        <sty:FluentAvaloniaTheme PreferUserAccentColor="True" />
    </Application.Styles>
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <FontFamily x:Key="UbuntuMono">avares://Tkmm.Launcher/Assets/Fonts/UbuntuMono/#Ubuntu Mono</FontFamily>
                    <FontFamily x:Key="HyliaGlyph">avares://Tkmm.Launcher/Assets/Fonts/Nin-ZeldaGlyphs-v2/Nin-ZeldaGlyphs-v2-Deco.bfotf.ttf#Nin-Zelda Glyphs v2, Deco</FontFamily>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
