<UserControl x:Class="Tcml.Views.Pages.HomePageView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:pages="using:Tcml.ViewModels.Pages"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:DataType="pages:HomePageViewModel"
             mc:Ignorable="d">
	<Grid>
		<Button Command="{Binding SetStatusCommand}" Content="Set Status" HorizontalAlignment="Right" Margin="0,0,50,0"/>
		<TextBlock HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,40,0,0" FontSize="60" Foreground="#0078c9" FontFamily="{StaticResource TCMLTitleFont}">
			TCML
		</TextBlock>
		<TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="20,0,0,90" FontSize="30" Foreground="#FFFFFF">
			Description
		</TextBlock>
	</Grid>
</UserControl>
