<UserControl x:Class="Tcml.Views.Pages.HomePageView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:pages="using:Tcml.ViewModels.Pages"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:DataType="pages:HomePageViewModel"
             mc:Ignorable="d">
	<Grid HorizontalAlignment="Center" VerticalAlignment="Center">
		<Canvas>
			<TextBlock Text="TCML" Canvas.Left="-300" Canvas.Top="-170" />
		</Canvas>
		<Button Command="{Binding SetStatusCommand}" Content="Set Status" />
	</Grid>
</UserControl>
